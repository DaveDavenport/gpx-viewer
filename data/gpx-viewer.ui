<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="gpx_viewer_window">
    <property name="title" translatable="yes">GPX Viewer</property>
