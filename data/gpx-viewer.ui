              <object class="GtkSpinButton" id="map_zoom_level">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="tooltip_text" translatable="yes">Change the zoom level of the map.</property>
                <property name="max_length">2</property>
                <property name="width_chars">5</property>
                <property name="primary_icon_stock">gtk-zoom-in</property>
                <property name="adjustment">map_zoom_level_adjustment</property>
                <property name="climb_rate">1</property>
                <signal name="value_changed" handler="map_zoom_level_change_value_cb"/>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkSpinButton" id="smooth_factor">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="tooltip_text" translatable="yes">Change the smoothing factor used when plotting the speed graph.</property>
                <property name="invisible_char">&#x25CF;</property>
                <property name="adjustment">smooth_factor_adjustment</property>
                <property name="climb_rate">1</property>
                <property name="numeric">True</property>
                <signal name="value_changed" handler="smooth_factor_change_value_cb"/>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="position">2</property>
              </packing>
            </child>
