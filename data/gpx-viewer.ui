  <object class="GtkAccelGroup" id="accelgroup1"/>
  <object class="GtkListStore" id="routes_store">
    <columns>
      <!-- column-name Name -->
      <column type="gchararray"/>
      <!-- column-name Route -->
      <column type="gpointer"/>
    </columns>
  </object>
  <object class="GtkSizeGroup" id="heading_label_sizegroup">
    <widgets>
      <widget name="num_points_label"/>
      <widget name="max_speed_label"/>
      <widget name="moving_average_time_label"/>
      <widget name="moving_average_label"/>
      <widget name="average_label"/>
      <widget name="duration_label"/>
      <widget name="distance_label"/>
    </widgets>
  </object>
  <object class="GtkAdjustment" id="map_zoom_level_adjustment">
    <property name="value">5</property>
    <property name="lower">1</property>
    <property name="upper">19</property>
    <property name="step_increment">1</property>
    <property name="page_increment">1</property>
    <property name="page_size">1</property>
  </object>
  <object class="GtkAdjustment" id="smooth_factor_adjustment">
    <property name="value">4</property>
    <property name="lower">1</property>
    <property name="upper">100</property>
    <property name="step_increment">1</property>
    <property name="page_increment">1</property>
    <property name="page_size">1</property>
  </object>
</interface>
